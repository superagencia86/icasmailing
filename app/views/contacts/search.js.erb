  $("#contacts-list").html("<%= escape_javascript render(:partial => 'contact', :collection => @contacts) %>");
  <% if @contacts.size == 30 %>
    $("#contacts-list").append("<b>La b√∫squeda se ha limitado a 30 elementos.</b>");
  <% end %>
<% else %>
  $("#contacts-list").html("<%= escape_javascript render(:partial => 'common/empty_search') %>");
<% end %>

